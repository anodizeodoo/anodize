<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="hr_scholarship_grade_form_inherit" model="ir.ui.view">
            <field name="name">hr.scholarship.grade.form.inherit</field>
            <field name="model">hr.scholarship.grade</field>
            <field name="inherit_id" ref="erp_hr_relatives_scholarship.hr_scholarship_grade_form"/>
            <field name="arch" type="xml">
                <xpath expr="//group[@name='date_capture']" position="after">
                    <group>
                        <field name="salary_rule"/>
                    </group>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
