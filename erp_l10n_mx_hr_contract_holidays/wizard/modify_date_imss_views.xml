<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_modify_date_imss_form" model="ir.ui.view">
            <field name="name">modify.date.imss.wizard.form</field>
            <field name="model">modify.date.imss.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="l10n_mx_date_imss"/>
                            </group>
                            <group>
                                <field name="contract_id" invisible="1"/>
                            </group>
                        </group>
                    </sheet>
                    <footer>
                        <button name="update_date_imss" string="Update" type="object" class="oe_highlight"/>
                        <button special="cancel" string="Cancel"/>
                </footer>
                </form>
            </field>
        </record>

        <record id="modify_date_imss_action" model="ir.actions.act_window">
            <field name="name">Modify date IMSS</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">modify.date.imss.wizard</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>


    </data>
</odoo>
